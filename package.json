{
  "name": "quasar-project",
  "version": "0.0.54",
  "description": "A Quasar Project",
  "productName": "Quasar App",
  "author": "tanglinchuanz@163.com <tanglinchuanz@163.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "vitest run",
    "dev": "quasar dev",
    "dev:pwa": "quasar dev -m pwa",
    "dev:android": "quasar dev -m capacitor -T android",
    "build": "quasar build",
    "build:pwa": "quasar build -m pwa",
    "build:android": "quasar build -m capacitor -T android --ide",
    "patch": "bump patch -t -a -p package.json package-lock.json src-capacitor/package.json src-capacitor/package-lock.json",
    "minor": "bump minor -t -a -p package.json package-lock.json src-capacitor/package.json src-capacitor/package-lock.json",
    "major": "bump major -t -a -p package.json package-lock.json src-capacitor/package.json src-capacitor/package-lock.json",
    "version": "npm install && cd src-capacitor && npm install && cd ..",
    "test:unit": "vitest",
    "test:unit:ci": "vitest run"
  },
  "dependencies": {
    "@aparajita/capacitor-dark-mode": "^5.0.2",
    "@capacitor/app-launcher": "^6.0.2",
    "@capacitor/barcode-scanner": "^1.0.1",
    "@capacitor/camera": "^6.0.2",
    "@capacitor/geolocation": "^6.0.1",
    "@capacitor/network": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "@capawesome/capacitor-live-update": "^6.3.0",
    "@capgo/inappbrowser": "^6.4.4",
    "@ionic/pwa-elements": "^3.3.0",
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "capacitor-webview-update": "^0.0.2",
    "ln-capacitor-wifi": "^0.3.0-beta",
    "pinia": "^2.0.11",
    "quasar": "^2.16.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.4.18",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@quasar/app-vite": "^2.0.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.1.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "vite-plugin-checker": "^0.7.2",
    "workbox-build": "^7.1.1",
    "workbox-cacheable-response": "^7.1.0",
    "workbox-core": "^7.1.0",
    "workbox-expiration": "^7.1.0",
    "workbox-precaching": "^7.1.0",
    "workbox-routing": "^7.1.0",
    "workbox-strategies": "^7.1.0",
    "@vue/test-utils": "^2.4.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": "^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
